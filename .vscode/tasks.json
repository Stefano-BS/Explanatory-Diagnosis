{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "SC",
            "command": "C:\\MinGW\\bin\\gcc.exe",
            "args": [
                "-xc",
                "-std=c99",
                "-D\"DEBUG_MODE=0\"",
                "-D\"LANG='e'\"",
                "-g",
                "${fileDirname}\\header.h", "${fileDirname}\\Explainer.c", "${fileDirname}\\SpaceMaker.c",
                "${fileDirname}\\Diagnoser.c", "${fileDirname}\\Printer.c", "${fileDirname}\\Parser.c",
                "${fileDirname}\\DataStructures.c", "${fileDirname}\\Main.c",
                "-o",
                "${fileDirname}\\SC.exe",
                "-lm"
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            }
        }
    ],
    "version": "2.0.0"
}